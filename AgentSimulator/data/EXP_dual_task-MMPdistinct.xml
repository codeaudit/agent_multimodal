<?xml version="1.0" encoding="utf-8"?>
<ExperimentWrapper xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <RoomClearingExperiment>
    <gridCollision>false</gridCollision>
    <substrateDescriptionFilename>hardmaze_substrate-preference.xml</substrateDescriptionFilename>
    <adaptableANN>false</adaptableANN>
    <modulatoryANN>false</modulatoryANN>
     <normalizeWeights>false</normalizeWeights>
    <populationSize>300</populationSize>
    <headingNoise>0</headingNoise>
    <effectorNoise>0</effectorNoise>
    <sensorNoise>0</sensorNoise>
    <timesToRunEnvironments>1</timesToRunEnvironments>

    <evaluationTime>45</evaluationTime>
    <timestep>0.2</timestep>
    <explanatoryText>Multi-Agent Experiment</explanatoryText>
    <scriptFile />
    <robotModelName>MazeRobotPieSlice</robotModelName>
    <fitnessFunctionName>DualTaskFitness</fitnessFunctionName>
    <behaviorCharacterizationName>null</behaviorCharacterizationName>
    <environmentName>ENV_dual_task.xml</environmentName>
    <overrideDefaultSensorDensity>false</overrideDefaultSensorDensity>

    <rangefinderSensorDensity>5</rangefinderSensorDensity>
    <useScript>false</useScript>
    <multipleEnvironments>true</multipleEnvironments>
    <evolveSubstrate>false</evolveSubstrate>
    <modulatory>false</modulatory>
    <numberRobots>1</numberRobots>

    <homogeneousTeam>false</homogeneousTeam>
    <agentsVisible>true</agentsVisible>
    <agentsCollide>true</agentsCollide>
	
    <multibrain>true</multibrain>
    <numBrains>1</numBrains>
    <mmpRate>0.01</mmpRate>
    <ungeometricBrains>true</ungeometricBrains>
    <preferenceNeurons>true</preferenceNeurons>
    
    <dedicatedPreferenceOutputInCPPN>true</dedicatedPreferenceOutputInCPPN>
    
  </RoomClearingExperiment>
</ExperimentWrapper>
